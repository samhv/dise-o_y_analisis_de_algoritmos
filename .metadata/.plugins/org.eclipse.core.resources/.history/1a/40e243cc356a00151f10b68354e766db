	}

	@Override
	protected int search(String pattern, String text) {
		
		int cantidad = 0;
		for(int i = 0; i < text.length() ; i++){
			for(int j = 0 ; j < pattern.length() && (i+j) < text.length(); j++){
				
			}
		}
		
		return cantidad;
	}
}
