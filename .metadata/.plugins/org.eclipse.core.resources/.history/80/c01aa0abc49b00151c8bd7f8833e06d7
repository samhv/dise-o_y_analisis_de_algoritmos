public class C {

	public static void main(String[] args) {
		A a = new A();
		A b = new A();
		A c = new A();
		a.a = b;
		b.a = c;

		System.out.println(ObjectSizeFetcher.getObjectSize(a));
		System.out.println(ObjectSizeFetcher.getObjectSize(b));
		System.out.println(ObjectSizeFetcher.getObjectSize(c));

	}
}